<nav class="mainnav">
  <ul class="mainnav__list container">
    <% for (var icon in theme.menu) { %>
    <li class="mainnav__item">
      <a class="mainnav__link" href="<%- url_for(theme.menu[icon]) %>" title="<%= _p(icon) %>">
        <%- partial('_partial/svg', { id: icon }) %>
        <span><%= _p(icon) %></span>
      </a>
    </li>
    <% } %>
  </ul>
  <div class="mainnav__search" hidden>
    <%- search_form() %>
  </div>
</nav>
